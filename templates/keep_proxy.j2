{% if proxy_http_proxy is defined %}
Defaults  env_keep +="HTTP_PROXY"
Defaults  env_keep +="http_proxy"
{% endif %}
{% if proxy_https_proxy is defined %}
Defaults  env_keep +="HTTPS_PROXY"
Defaults  env_keep +="https_proxy"
{% endif %}
{% if proxy_no_proxy is defined %}
Defaults  env_keep +="NO_PROXY"
Defaults  env_keep +="no_proxy"
{% endif %}
